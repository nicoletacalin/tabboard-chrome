{
  "manifest_version": 2,
  "name": "TaBoard",
  "version": "1.0.2",
  "description": "Clear Space - Clear Mind",
  "homepage_url": "https://taboard.herokuapp.com",
  "icons": {
    "128": "icon/icon_128.png"
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "tabs",
    "storage",
    "identity"
  ],
  "background": {
    "scripts": [
      "./js/background.js",
      "./js/jsrasign-all-min.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup-sign-in.html"
  },
  "oauth2": {
    "client_id": "971672027408-v2fhakhcl32m5qg9v6mhs5hmnprpp43j.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/userinfo.email"]
  }
}
